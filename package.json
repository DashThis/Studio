{
  "name": "@dashthis/studio-app",
  "version": "3.4.5",
  "private": true,
  "license": "UNLICENSED",
  "main": "build/electron.js",
  "homepage": "./",
  "description": "Accounts management for DashThis app",
  "scripts": {
    "release-win": "electron-builder --win --publish=always"
  },
  "devDependencies": {
    "dotenv": "16.0.0",
    "electron": "22.0.2",
    "electron-builder": "23.6.0",
    "electron-builder-notarize": "1.5.0",
    "electron-notarize": "1.2.2"
  },
  "author": {
    "name": "DashThis",
    "email": "info@dashthis.com",
    "url": "https://dashthis.com"
  },
  "build": {
    "appId": "com.studio3",
    "productName": "Studio 3",
    "copyright": "Copyright Â© 2020 DashThis",
    "dmg": {
      "sign": false
    },
    "afterSign": "electron-builder-notarize",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "x64",
          "arm64"
        ]
      },
      "hardenedRuntime": true,
      "gatekeeperAssess": false,
      "entitlements": "entitlements.mac.plist",
      "entitlementsInherit": "entitlements.mac.plist",
      "category": "com.dashthis.studio.stats"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
