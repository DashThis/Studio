{
  "name": "@dt/studio",
  "version": "3.0.0",
  "private": true,
  "license": "UNLICENSED",
  "main": "public/electron.js",
  "sideEffects": false,
  "homepage": "./",
  "resolutions": {
    "//node-gyp": "5.0.0"
  },
  "description": "Accounts management for DashThis app",
  "scripts": {
    "package": "gulp package",
    "electron-pack": "electron-builder -m",
    "storybook": "concurrently \"start-storybook -p 9009 -s public\" \"yarn build-style-dev\"",
    "dev": "yarn build-studio-electron-dev && concurrently \"yarn build-style-dev\" \"BROWSER=none yarn webpack-dev\" \"wait-on http://localhost:9000 && electron .\"",
    "up": "yarn upgrade-interactive --latest",
    "lint": "tslint -c tslint.json 'src/**/*.ts{,x}'",
    "typeCheck-": "tsc --noEmit",
    "electron-postinstall": "electron-builder install-app-deps",
    "preelectron-pack": "yarn build-studio-electron && yarn webpack-build",
    "webpack-dev": "cross-env NODE_OPTIONS=--experimental-worker webpack-dev-server --mode=development",
    "webpack-build": "cross-env NODE_OPTIONS=--experimental-worker webpack --mode=production",
    "build-storybook": "build-storybook -s public",
    "build-studio-electron-dev": "yarn dist-dep && yarn workspace @dt/studio-electron dev",
    "build-studio-electron": "yarn build-style && yarn dist-dep  && yarn workspace @dt/studio-electron build",
    "build-style-dev": "yarn --cwd ../../../DashThis.Styles run dev",
    "build-style": "yarn --cwd ../../../DashThis.Styles run build",
    "dist-dep": "yarn --cwd ../.. run dist"
  },
  "dependencies": {
    "@amcharts/amcharts4": "^4.7.7",
    "@apollo/react-hooks": "^3.1.3",
    "@dt/common": "1.0.0",
    "@dt/studio-electron": "1.0.0",
    "@dt/ui": "1.0.0",
    "axios": "^0.19.0",
    "classnames": "^2.2.6",
    "dayjs": "^1.8.16",
    "email-addresses": "^3.0.3",
    "format-message": "^6.2.3",
    "formik": "^2.0.3",
    "graphql-tag": "^2.10.1",
    "graphql-tools": "^4.0.5",
    "http-status-codes": "^1.3.2",
    "immer": "^5.0.0",
    "keycode": "^2.2.0",
    "marked": "^0.7.0",
    "md5": "^2.2.1",
    "numeral": "^2.0.6",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-html-parser": "^2.0.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "3.2.0",
    "react-virtualized": "^9.21.1",
    "redux": "^4.0.4",
    "uuid": "^3.3.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/plugin-proposal-object-rest-spread": "^7.6.2",
    "@babel/preset-typescript": "^7.6.0",
    "@dt/studio-electron": "1.0.0",
    "@rescripts/cli": "^0.0.13",
    "@rescripts/rescript-env": "^0.0.11",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-info": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react": "^5.2.5",
    "@testing-library/react-hooks": "^3.1.0",
    "@types/classnames": "^2.2.9",
    "@types/enzyme": "^3.10.3",
    "@types/jest": "^24.0.19",
    "@types/marked": "^0.6.5",
    "@types/numeral": "^0.0.26",
    "@types/react": "^16.9.9",
    "@types/react-dom": "^16.9.2",
    "@types/react-html-parser": "^2.0.1",
    "@types/react-redux": "^7.1.5",
    "@types/react-router-dom": "^5.1.0",
    "@types/react-virtualized": "^9.21.5",
    "@types/storybook__react": "^4.0.2",
    "awesome-typescript-loader": "^5.2.1",
    "babel-loader": "^8.0.6",
    "concurrently": "^5.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "copyfiles": "^2.1.1",
    "electron": "^7.0.0",
    "electron-builder": "^22.1.0",
    "electron-rebuild": "^1.8.6",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "graphql-tag": "^2.10.1",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.9.0",
    "jest-transform-graphql": "^2.1.0",
    "lerna": "3.18.3",
    "react-docgen-typescript-loader": "^3.3.0",
    "react-svg-loader": "^3.0.3",
    "react-test-renderer": "^16.11.0",
    "source-map-loader": "^0.2.4",
    "ts-essentials": "^3.0.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.4",
    "waait": "^1.0.5",
    "wait-on": "^3.3.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.9.0"
  },
  "author": {
    "name": "DashThis",
    "email": "info@dashthis.com",
    "url": "https://dashthis.com"
  },
  "build": {
    "publish": [
      {
        "provider": "generic",
        "url": "https://dashthis.blob.core.windows.net/cdn/studio/v2",
        "channel": "latest"
      }
    ],
    "appId": "com.dashthisstudio",
    "productName": "DashThis Studio",
    "copyright": "Copyright Â© 2019 DashThis",
    "mac": {
      "category": "com.dashthis.studio.stats"
    },
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "directories": {
      "buildResources": "assets"
    }
  }
}
